            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:background="@color/colorAccent"/>

        <TextView
            android:layout_marginTop="15dp"
            android:text="STEEL COIL"
            android:layout_width="130dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:layout_height="wrap_content"
            android:textColor="@color/colorAccent"
            android:textSize="12sp"
            android:id="@+id/sumary_name"
            android:fontFamily="@font/montserrat_bold"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <LinearLayout
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/ln1_item">

                <TextView
                    android:id="@+id/title1_item"
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorGrey"
                    android:text="Destination"
                    android:textSize="10dp"
                    android:textStyle="italic"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/item1_item"
                    android:textColor="@color/colorTextbooking"
                    android:textAllCaps="true"
                    android:textSize="11dp"
                    android:text="MYPKG"
                    android:fontFamily="@font/montserrat_bold"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/ln2_item"
                app:layout_constraintStart_toEndOf="@id/ln1_item"
