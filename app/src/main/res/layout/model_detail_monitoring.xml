                android:layout_width="140dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@id/ln1_item"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/ln3_item">

                <TextView
                    android:id="@+id/title3_item"
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorGrey"
                    android:text="Hatch No"
                    android:textSize="10dp"
                    android:textStyle="italic"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/item3_item"
                    android:textColor="@color/colorTextbooking"
                    android:textAllCaps="true"
                    android:textSize="11dp"
                    android:text="1"
                    android:fontFamily="@font/montserrat_bold"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@id/ln1_item"
                android:id="@+id/ln4_item"
                app:layout_constraintStart_toStartOf="@id/ln2_item"
                android:layout_marginTop="8dp">

                <TextView
                    android:id="@+id/title4_item"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="10dp"
                    android:textColor="@color/colorGrey"
                    android:textStyle="italic"
                    android:text="Hatch Side"/>

                <TextView
                    android:ellipsize="end"
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorTextbooking"
                    android:text="N"
                    android:maxLines="1"
                    android:textSize="11dp"
                    android:textAllCaps="true"
                    android:fontFamily="@font/montserrat_bold"
                    android:id="@+id/item4_item"/>


            </LinearLayout>

            <LinearLayout
                app:layout_constraintTop_toBottomOf="@id/ln3_item"
                android:layout_marginTop="8dp"
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
