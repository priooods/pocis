                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintStart_toEndOf="@id/ln_pcs0">

                <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorGrey"
                    android:text="Tonnage"
                    android:textSize="10dp"
                    android:textStyle="italic"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/item_tonnage0"
                    android:textColor="@color/colorTextbooking"
                    android:textAllCaps="true"
                    android:textSize="11dp"
                    android:text="89"
                    android:fontFamily="@font/montserrat_bold"/>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ln_ritase"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorGrey"
                    android:text="Ritase"
                    android:textSize="10dp"
                    android:textStyle="italic"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/item_ritase"
                    android:textColor="@color/colorTextbooking"
                    android:textAllCaps="true"
                    android:textSize="11dp"
                    android:text="89"
                    android:fontFamily="@font/montserrat_bold"/>


            </LinearLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:layout_width="match_parent"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="Remaining Loading"
            android:textColor="@color/colorGreyDark"
            android:layout_height="wrap_content"
            android:textSize="11dp"
            android:fontFamily="@font/montserrat_bold"
            android:layout_marginTop="12dp"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <LinearLayout
                android:id="@+id/ln_pcs1"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorGrey"
                    android:text="Pcs"
                    android:textSize="10dp"
                    android:textStyle="italic"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/item_pcs1"
                    android:textColor="@color/colorTextbooking"
                    android:textAllCaps="true"
                    android:textSize="11dp"
                    android:text="89"
                    android:fontFamily="@font/montserrat_bold"/>


            </LinearLayout>

            <LinearLayout
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorGrey"
                    android:text="Tonnage"
                    android:textSize="10dp"
                    android:textStyle="italic"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/item_tonnage1"
                    android:textColor="@color/colorTextbooking"
                    android:textAllCaps="true"
                    android:textSize="11dp"
                    android:text="89"
                    android:fontFamily="@font/montserrat_bold"/>


            </LinearLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:layout_width="match_parent"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="Ritase"
            android:textColor="@color/colorGreyDark"
            android:layout_height="wrap_content"
            android:textSize="11dp"
            android:fontFamily="@font/montserrat_bold"
            android:layout_marginTop="12dp"/>

        <TextView
            android:layout_width="match_parent"
            android:ellipsize="end"
            android:maxLines="1"
            android:id="@+id/ritase"
            android:text="0"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="wrap_content"
            android:textSize="11dp"
            android:fontFamily="@font/montserrat_bold"
            android:layout_marginTop="5dp"/>

    </LinearLayout>

    <LinearLayout
        android:visibility="gone"
        android:id="@+id/model_stowage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_marginTop="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11dp"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/montserrat_bold"
            android:textColor="@color/colorGreyDark"
            android:text="Top View"/>

        <ImageView
            android:layout_marginTop="13dp"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:background="@color/colorGrey"
            android:id="@+id/top_view"/>

        <TextView
            android:layout_marginTop="14dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11dp"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/montserrat_bold"
            android:textColor="@color/colorGreyDark"
            android:text="Side View"/>

        <ImageView
            android:layout_marginTop="13dp"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:background="@color/colorGrey"
            android:id="@+id/side_view"/>


    </LinearLayout>

    <LinearLayout
        android:paddingBottom="15dp"
        android:visibility="gone"
        android:id="@+id/model_sumary"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <View
