<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/colorWhite"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/a"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_alignParentStart="true"
            android:layout_width="40dp"
            android:layout_marginStart="10dp"
            android:layout_height="35dp"
            android:src="@drawable/icon_back"
            android:id="@+id/btn_back"/>

        <LinearLayout
            android:layout_centerHorizontal="true"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Loading/Unloading Progress"
                android:textColor="@color/colorPrimaryDark"
                android:layout_centerHorizontal="true"
                android:textSize="14dp"
                android:fontFamily="@font/montserrat_bold"/>

            <TextView
                android:id="@+id/title2"
                android:layout_marginTop="4dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Operational Monitoring"
                android:fontFamily="@font/montserrat_bold"
                android:textColor="@color/colorGrey"
                android:textSize="9dp"/>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:id="@+id/aa"
        android:layout_below="@id/a"
        android:paddingHorizontal="15dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp">

        <LinearLayout
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/ln_top1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:orientation="vertical">

            <TextView
                android:id="@+id/title_top1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10sp"
                android:text="Vessel Name"
                android:fontFamily="@font/montserrat_reguler"
                android:textColor="@color/colorPrimaryDark"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorPrimaryDark"
                android:fontFamily="@font/montserrat_bold"
                android:textSize="12sp"
                android:textAllCaps="true"
                android:layout_marginTop="4dp"
                android:id="@+id/item_top1"/>

        </LinearLayout>

        <LinearLayout
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/ln_top2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:gravity="end"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="9dp"
                android:id="@+id/title_top2"
                android:text="Booking No"
                android:fontFamily="@font/montserrat_reguler"
                android:textColor="@color/colorPrimaryDark"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorGrey"
                android:fontFamily="@font/montserrat_bold"
                android:textSize="12sp"
                android:textStyle="bold"
                android:layout_marginTop="4dp"
                android:id="@+id/item_top2"/>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout
        android:visibility="gone"
        android:id="@+id/ln_vesel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/aa">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ab"
            android:layout_marginTop="15dp"
            android:textSize="12sp"
            android:text="Information"
            android:textColor="@color/colorPrimaryDark"
            android:fontFamily="@font/montserrat_bold"
            android:layout_marginStart="15dp"/>

        <com.google.android.material.tabs.TabLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tab_vessel"
            android:layout_below="@id/ab"
            app:tabSelectedTextColor="@color/colorPrimaryDark"
            app:tabTextAppearance="@style/TablayoutStyle"
            app:tabMode="scrollable"
            app:tabTextColor="@color/colorPrimaryDarkLow"
            app:tabGravity="center"
            app:tabIndicatorHeight="3dp"
            android:layout_marginTop="8dp"/>

        <androidx.viewpager.widget.ViewPager
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/tab_vessel"
            android:id="@+id/viewpager"/>

    </RelativeLayout>

    <RelativeLayout
        android:visibility="gone"
        android:paddingHorizontal="15dp"
        android:id="@+id/ln_progress"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/aa">

        <LinearLayout
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/ln_p1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:orientation="vertical">

            <TextView
                android:id="@+id/title_p1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10sp"
                android:text="@string/schedule"
                android:fontFamily="@font/montserrat_reguler"
                android:textColor="@color/colorPrimaryDark"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorGrey"
                android:fontFamily="@font/montserrat_bold"
                android:textSize="12sp"
                android:textAllCaps="true"
                android:layout_marginTop="4dp"
                android:id="@+id/item_p1"/>

        </LinearLayout>

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/ln_p1"
            android:layout_marginTop="10dp">

            <RelativeLayout
                android:layout_marginBottom="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ln_info0"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardElevation="0dp"
                        app:cardCornerRadius="8dp">

                        <RelativeLayout
                            android:padding="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#F9F9F9">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <LinearLayout
                                    app:layout_constraintTop_toTopOf="parent"
                                    android:layout_marginTop="8dp"
                                    android:layout_width="140dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintStart_toStartOf="parent"
                                    android:id="@+id/ln_info1">

                                    <TextView
                                        android:id="@+id/title_info1"
                                        android:layout_width="wrap_content"
                                        android:textColor="@color/colorGrey"
                                        android:text="@string/voyage"
                                        android:textSize="9sp"
                                        android:textStyle="italic"
                                        android:layout_height="wrap_content"/>

                                    <TextView
                                        android:maxLines="1"
                                        android:ellipsize="end"
                                        android:layout_width="140dp"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/item_info1"
                                        android:textColor="@color/colorTextbooking"
                                        android:textAllCaps="true"
                                        android:textSize="10sp"
                                        android:fontFamily="@font/montserrat_bold"/>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="130dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    android:id="@+id/ln_info2"
                                    app:layout_constraintStart_toEndOf="@id/ln_info1"
                                    android:layout_marginTop="8dp">

                                    <TextView
                                        android:id="@+id/title_info2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textSize="9sp"
                                        android:textColor="@color/colorGrey"
                                        android:textStyle="italic"
                                        android:text="@string/jetty"/>

                                    <TextView
                                        android:maxLines="1"
                                        android:ellipsize="end"
                                        android:layout_width="140dp"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/colorTextbooking"
                                        android:textSize="10sp"
                                        android:textAllCaps="true"
                                        android:fontFamily="@font/montserrat_bold"
                                        android:id="@+id/item_info2"/>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_marginTop="8dp"
                                    android:layout_width="140dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintTop_toBottomOf="@id/ln_info1"
                                    app:layout_constraintStart_toStartOf="parent"
                                    android:id="@+id/ln_info3">

                                    <TextView
                                        android:id="@+id/title_info3"
                                        android:layout_width="wrap_content"
                                        android:textColor="@color/colorGrey"
                                        android:text="@string/est_betring"
                                        android:textSize="9dp"
                                        android:textStyle="italic"
                                        android:layout_height="wrap_content"/>

                                    <TextView
                                        android:maxLines="1"
                                        android:ellipsize="end"
                                        android:layout_width="140dp"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/item_info3"
                                        android:textColor="@color/colorTextbooking"
                                        android:textAllCaps="true"
                                        android:textSize="10dp"
                                        android:text="BG. LKH 3883"
                                        android:fontFamily="@font/montserrat_bold"/>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintTop_toBottomOf="@id/ln_info1"
                                    android:id="@+id/ln_info4"
                                    app:layout_constraintStart_toStartOf="@id/ln_info2"
                                    android:layout_marginTop="8dp">

                                    <TextView
                                        android:id="@+id/title_info4"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textSize="9dp"
                                        android:textColor="@color/colorGrey"
                                        android:textStyle="italic"
                                        android:text="@string/act_betring"/>

                                    <TextView
                                        android:ellipsize="end"
                                        android:layout_width="130dp"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/colorTextbooking"
                                        android:text="CNEE"
                                        android:maxLines="1"
                                        android:textSize="10dp"
                                        android:textAllCaps="true"
                                        android:fontFamily="@font/montserrat_bold"
                                        android:id="@+id/item_info4"/>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_marginTop="8dp"
                                    android:layout_width="140dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintTop_toBottomOf="@id/ln_info3"
                                    app:layout_constraintStart_toStartOf="parent"
                                    android:id="@+id/ln_info5">

                                    <TextView
                                        android:id="@+id/title_info5"
                                        android:layout_width="wrap_content"
                                        android:textColor="@color/colorGrey"
                                        android:text="@string/est_depart"
                                        android:textSize="9dp"
                                        android:textStyle="italic"
                                        android:layout_height="wrap_content"/>

                                    <TextView
                                        android:maxLines="1"
                                        android:ellipsize="end"
                                        android:layout_width="140dp"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/item_info5"
                                        android:textColor="@color/colorTextbooking"
                                        android:textAllCaps="true"
                                        android:textSize="10dp"
                                        android:text="BG. LKH 3883"
                                        android:fontFamily="@font/montserrat_bold"/>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintTop_toBottomOf="@id/ln_info3"
                                    android:id="@+id/ln_info6"
                                    app:layout_constraintStart_toStartOf="@id/ln_info4"
                                    android:layout_marginTop="8dp">

                                    <TextView
                                        android:id="@+id/title_info6"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textSize="9dp"
                                        android:textColor="@color/colorGrey"
                                        android:textStyle="italic"
                                        android:text="@string/act_depart"/>

                                    <TextView
                                        android:ellipsize="end"
                                        android:layout_width="130dp"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/colorTextbooking"
                                        android:text="CNEE"
                                        android:maxLines="1"
                                        android:textSize="10dp"
                                        android:textAllCaps="true"
                                        android:fontFamily="@font/montserrat_bold"
                                        android:id="@+id/item_info6"/>


                                </LinearLayout>

                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>


                </androidx.constraintlayout.widget.ConstraintLayout>

                <LinearLayout
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:id="@+id/ln_cctv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="15dp"
                    android:padding="10dp"
                    android:background="#F9F9F9"
                    android:layout_below="@id/ln_info0">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="CCTV"
                            android:layout_centerVertical="true"
                            android:textSize="12sp"
                            android:fontFamily="@font/montserrat_bold"
                            android:textColor="@color/colorPrimaryDark"/>

                        <ImageView
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_botom"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/icon_cctv"
                            android:contentDescription="@string/icon_back" />

                    </RelativeLayout>

                    <net.cachapa.expandablelayout.ExpandableLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:el_duration="300"
                        app:el_parallax="1"
                        android:id="@+id/expand_cctv">

                        <WebView
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="170dp"
                            android:id="@+id/content_cctv"/>

                    </net.cachapa.expandablelayout.ExpandableLayout>

                </LinearLayout>

                <LinearLayout
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:id="@+id/ln_contact"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="15dp"
                    android:padding="10dp"
                    android:background="#F9F9F9"
                    android:layout_below="@id/ln_cctv">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Contact Person"
                            android:layout_centerVertical="true"
                            android:textSize="12dp"
                            android:fontFamily="@font/montserrat_bold"
                            android:textColor="@color/colorPrimaryDark"/>

                        <ImageView
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_botom"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/icon_contact"/>

                    </RelativeLayout>

                    <net.cachapa.expandablelayout.ExpandableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:el_duration="400"
                        app:el_parallax="0.8"
                        android:id="@+id/expand_contact">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <androidx.recyclerview.widget.RecyclerView
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:nestedScrollingEnabled="true"
                                android:layout_height="wrap_content"
                                android:id="@+id/list_contact_agent"/>

                            <androidx.recyclerview.widget.RecyclerView
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:nestedScrollingEnabled="true"
                                android:layout_height="wrap_content"
                                android:id="@+id/list_contact_pbm"/>

                        </LinearLayout>


                    </net.cachapa.expandablelayout.ExpandableLayout>

                </LinearLayout>

                <LinearLayout
                    android:focusableInTouchMode="false"
                    android:focusable="false"
                    android:id="@+id/ln_actual"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="15dp"
                    android:padding="10dp"
                    android:background="#F9F9F9"
                    android:layout_below="@id/ln_contact">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Actual Truck Monitoring"
                            android:layout_centerVertical="true"
                            android:textSize="12dp"
                            android:fontFamily="@font/montserrat_bold"
                            android:textColor="@color/colorPrimaryDark"/>

                        <ImageView
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_botom"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/icon_actual"/>

                    </RelativeLayout>

                    <net.cachapa.expandablelayout.ExpandableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:el_duration="400"
                        app:el_parallax="0.8"
                        android:id="@+id/expand_actual">

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:nestedScrollingEnabled="true"
                            android:layout_height="wrap_content"
                            android:id="@+id/list_actual"/>

                    </net.cachapa.expandablelayout.ExpandableLayout>

                </LinearLayout>

                <LinearLayout
                    android:focusableInTouchMode="false"
                    android:focusable="false"
                    android:id="@+id/ln_operational"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="15dp"
                    android:padding="10dp"
                    android:background="#F9F9F9"
                    android:layout_below="@id/ln_actual">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Actual Operational Monitoring"
                            android:layout_centerVertical="true"
                            android:textSize="12dp"
                            android:fontFamily="@font/montserrat_bold"
                            android:textColor="@color/colorPrimaryDark"/>

                        <ImageView
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_botom"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/icon_operational"/>

                    </RelativeLayout>

                    <net.cachapa.expandablelayout.ExpandableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:el_duration="400"
                        app:el_parallax="0.8"
                        android:id="@+id/expand_operational">

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:nestedScrollingEnabled="true"
                            android:layout_height="wrap_content"
                            android:id="@+id/list_operational"/>

                    </net.cachapa.expandablelayout.ExpandableLayout>

                </LinearLayout>

                <LinearLayout
                    android:focusableInTouchMode="false"
                    android:focusable="false"
                    android:id="@+id/ln_actual_vesel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="15dp"
                    android:padding="10dp"
                    android:background="#F9F9F9"
                    android:layout_below="@id/ln_operational">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Actual Vessel In Progress"
                            android:layout_centerVertical="true"
                            android:textSize="12dp"
                            android:fontFamily="@font/montserrat_bold"
                            android:textColor="@color/colorPrimaryDark"/>

                        <ImageView
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_botom"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/icon_actual_vesel"/>

                    </RelativeLayout>

                    <net.cachapa.expandablelayout.ExpandableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:el_duration="400"
                        app:el_parallax="0.8"
                        android:id="@+id/expand_actual_vesel">

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/list_actual_vessel"
                            android:nestedScrollingEnabled="true"/>

                    </net.cachapa.expandablelayout.ExpandableLayout>

                </LinearLayout>

                <LinearLayout
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:id="@+id/ln_stowage"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="15dp"
                    android:padding="10dp"
                    android:background="#F9F9F9"
                    android:layout_below="@id/ln_actual_vesel">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Actual Stowage Monitoring"
                            android:layout_centerVertical="true"
                            android:textSize="12dp"
                            android:fontFamily="@font/montserrat_bold"
                            android:textColor="@color/colorPrimaryDark"/>

                        <ImageView
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_botom"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/icon_stowage"/>

                    </RelativeLayout>

                    <net.cachapa.expandablelayout.ExpandableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:el_duration="400"
                        app:el_parallax="0.8"
                        android:id="@+id/expand_stowage">

                        <LinearLayout
                            android:id="@+id/ln_ship_all"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Top View"
                                    android:fontFamily="@font/montserrat_bold"
                                    android:textColor="@color/colorTextbooking"/>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_marginTop="10dp"
                                    android:layout_height="wrap_content">


                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/top_side"
                                        android:scaleType="fitXY"
                                        app:layout_constraintTop_toTopOf="parent"/>

                                    <LinearLayout
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        android:layout_width="200dp"
                                        android:layout_height="100dp"
                                        android:orientation="vertical"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintTop_toTopOf="parent">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:textColor="@color/colorWhite"
                                            android:background="@color/colorRed"
                                            android:id="@+id/title_name_top"/>

                                        <androidx.recyclerview.widget.RecyclerView
                                            android:layout_width="match_parent"
                                            android:nestedScrollingEnabled="true"
                                            android:layout_height="match_parent"
                                            android:id="@+id/list_top_view"/>

                                    </LinearLayout>

                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <TextView
                                    android:layout_marginTop="15dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Side View"
                                    android:fontFamily="@font/montserrat_bold"
                                    android:textColor="@color/colorTextbooking"/>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_marginTop="10dp"
                                    android:layout_height="wrap_content">

                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/bot_side"
                                        android:scaleType="fitXY"
                                        app:layout_constraintTop_toTopOf="parent"/>

                                    <LinearLayout
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        android:layout_width="230dp"
                                        android:layout_height="120dp"
                                        android:orientation="vertical"
                                        app:layout_constraintTop_toTopOf="parent">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:textColor="@color/colorWhite"
                                            android:background="@color/colorRed"
                                            android:id="@+id/title_name_bot"/>

                                        <androidx.recyclerview.widget.RecyclerView
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/list_side_view"/>

                                    </LinearLayout>

                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </LinearLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:id="@+id/ln_ship_all_no"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="80dp"
                            android:orientation="vertical"
                            android:gravity="center">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/montserrat_bold"
                                android:text="Nothing Data"
                                android:textColor="@color/colorPrimaryDark"/>


                        </LinearLayout>

                    </net.cachapa.expandablelayout.ExpandableLayout>

                </LinearLayout>

                <LinearLayout
                    android:focusableInTouchMode="false"
                    android:focusable="false"
                    android:id="@+id/ln_sumary"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="15dp"
                    android:padding="10dp"
                    android:background="#F9F9F9"
                    android:layout_below="@id/ln_stowage">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Item Summary"
                            android:layout_centerVertical="true"
                            android:textSize="12dp"
                            android:fontFamily="@font/montserrat_bold"
                            android:textColor="@color/colorPrimaryDark"/>

                        <ImageView
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_botom"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/icon_sumary"/>

                    </RelativeLayout>

                    <net.cachapa.expandablelayout.ExpandableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:el_duration="400"
                        app:el_parallax="0.8"
                        android:id="@+id/expand_sumary">

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:nestedScrollingEnabled="true"
                            android:layout_height="wrap_content"
                            android:id="@+id/list_sumary"/>

                    </net.cachapa.expandablelayout.ExpandableLayout>

                </LinearLayout>

            </RelativeLayout>

        </androidx.core.widget.NestedScrollView>


    </RelativeLayout>

    <RelativeLayout
        android:visibility="gone"
        android:id="@+id/progress"
        android:background="@color/colorWhite"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/a">

        <ProgressBar
            android:id="@+id/p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/load"
            android:fontFamily="@font/montserrat_bold"
            android:textColor="@color/colorPrimaryDark"
            android:layout_below="@id/p"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="13dp"/>


    </RelativeLayout>

    <RelativeLayout
        android:layout_below="@id/a"
        android:visibility="gone"
        android:background="@color/colorWhite"
        android:id="@+id/detail_kosong"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_centerHorizontal="true"
            android:layout_marginTop="120dp"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:id="@+id/ww"
            android:src="@drawable/notfound"
            android:scaleType="fitXY"/>

        <TextView
            android:layout_below="@id/ww"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="50dp"
            android:layout_centerHorizontal="true"
            android:textColor="@color/colorAccent"
            android:fontFamily="@font/montserrat_bold"
            android:textSize="15sp"
            android:layout_marginTop="20dp"
            android:text="@string/error"
            android:gravity="center"/>

    </RelativeLayout>

</RelativeLayout>